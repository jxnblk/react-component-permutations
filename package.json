{
  "name": "react-component-permutations",
  "version": "1.0.0-beta1",
  "description": "Uses propTypes to create an array of React component props to display various permutations",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "mkdir -p dist && babel src --out-dir dist",
    "test": "standard && ava test/index.js"
  },
  "keywords": [
    "react",
    "style-guide",
    "documentation"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-geomicons": "^2.0.4",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "react-docgen": "^2.7.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "standard": {
    "ignore": [
